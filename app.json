{
  "expo": {
    "name": "Lift Log",
    "slug": "gym-log",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon.png",
    "scheme": "gymlog",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "fit",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icons/splash-icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0fccad8-ba43-4289-af35-cab6b5de230b"
      }
    },
    "android": {
      "package": "test.android.lift.log",
      "edgeToEdgeEnabled": true
    }
  }
}
